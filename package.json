{
  "name": "react-volume-viewer",
  "version": "1.0.0",
  "description": "Aframe container with custom controls for use in react applications",
  "author": "brown-ccv",
  "license": "MIT",
  "repository": "brown-ccv/react-volume-viewer",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "run-s build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "test": "cross-env CI=1 react-scripts test --env=jsdom && run-s test:unit test:lint test:build",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepublish": "npm run build",
    "lint": "prettier --write src example/src"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    

    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-url": "^6.1.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.59.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1"
  },
  "files": [
    "dist"
  ]
}
