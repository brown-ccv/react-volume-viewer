{
  "name": "react-volume-viewer",
  "version": "1.0.0",
  "description": "Aframe container with custom controls for use in react applications",
  "author": "brown-ccv",
  "license": "MIT",
  "repository": "brown-ccv/react-volume-viewer",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prepare": "run-s build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "test": "cross-env CI=1 react-scripts test --env=jsdom && run-s test:unit test:lint test:build",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepublish": "npm run build",
    "lint": "prettier --write src example/src"
  },
  "dependencies": {
    "@reach/listbox": "^0.16.2",
    "aframe": "git+https://github.com/brown-ccv/aframe.git",
    "aframe-event-set-component": "^5.0.0",
    "aframe-orbit-controls": "^1.3.0",
    "aframe-react": "^4.4.0",
    "d3-scale": "^4.0.2",
    "rc-slider": "^9.7.4",
    "styled-components": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-url": "^6.1.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.59.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.1"
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
